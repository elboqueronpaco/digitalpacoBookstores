{
  "name": "digitalpacobookstores",
  "version": "0.0.3",
  "description": "Creación de librería de javascript y css -correcion",
  "main": "dist/index.js",
  "scripts": {
    "lint": "standard --fix",
    "dev": "nodemon src/index.js --ext js --exec babel-node",
    "build": "babel src --ignore src/**/__tests__/ -d dist",
